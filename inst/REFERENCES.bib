@inbook{AlexCar,
author = {Schmidt, Alexandra M. and Nobre, Widemberg S.},
publisher = {John Wiley & Sons, Ltd},
isbn = {9781118445112},
title = {Conditional Autoregressive (CAR) Model},
booktitle = {Wiley StatsRef: Statistics Reference Online},
chapter = {Conditional Autoregressive (CAR) Model},
pages = {1-11},
doi = {https://doi.org/10.1002/9781118445112.stat08048},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/9781118445112.stat08048},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/9781118445112.stat08048},
year = {2018},
keywords = {areal data level, Bayesian inference, disease mapping, Gaussian Markov random fields, image analysis, lattice data, spatial statistics},
abstract = {Abstract Conditional autoregressive (CAR) models are useful to obtain a multivariate joint distributions of a random vector based on univariate conditional specifications. These conditional specifications are based on Markovian properties such that the conditional distribution of a component of the random vector depends only on a set of neighbors. Conditional autoregressive models are particular cases of Markov random fields. CAR models have been applied in different areas of science; some examples are image analysis, epidemiology, and agriculture. Typically, Gaussian CAR specifications are used as latent structures in hierarchical models for areal level data. In this case, the region of interest is divided into a set of disjoint areas and a CAR random effect is used to account for possible correlation among observations made across the different areas.}
}

