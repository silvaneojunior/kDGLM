author = {Omuto, C.T. and Vargas, R.R. and Alim, M.S. and Paron, P.},
keywords = {Animal and plant ecology ; Animal plant and microbial ecology ; arid lands ; Biological and medical sciences ; Climate ; Ecology ; Enhanced vegetation index ; Environmental science ; Environmental Sciences ; Environmental Sciences \& Ecology ; Fundamental and applied biological sciences. Psychology ; General aspects. Techniques ; Hydrology ; Land use ; Life Sciences \& Biomedicine ; Loss of vegetation ; Mathematical models ; Methods and techniques (sampling tagging trapping modelling...) ; Mixed-effects ; Modelling ; NDVI ; Normalized Difference Vegetation Index ; Physical geography ; Plant cover ; Population ; Population (statistical) ; rain ; Rainfall ; Remote sensing ; researchers ; Science \& Technology ; Soil classification ; soil types ; Statistical model ; Synecology ; Terrestrial ecosystems ; time series analysis ; Vegetation ; Vegetation (pathology) ; vegetation cover ; vegetation types},
}

@article{ZhangHeping2008JMoT,
issn = {0162-1459},
abstract = {Exposure to ambient pollutants at concentrations above defined standards is a risk factor for respiratory symptoms, especially in sensitive children. Many studies have been undertaken to monitor air quality and to assess its association with respiratory symptoms. We propose a joint mixed-effects regression model of time series measures and recurrent events to analyze the air quality and respiratory symptom data from the Yale Mothers and Infants Health Study. Three mothers' symptoms (runny nose, cough, and sore throat) and three infants' symptoms (runny nose, cough, and general sickness) were investigated. To alleviate the computational complexity, a two-stage maximum likelihood-based estimation procedure is introduced to estimate the parameters, and simulation studies are conducted to assess the validity of this estimation procedure. Our analysis reveals differences in the etiology of respiratory symptoms between mothers and infants. Most notably, coarse particles of mass between 2.5 and 10 μ in diameter increased the risks of mothers' runny nose and cough symptoms but had no significant effect on any of the three infants' symptoms. The sulfate level was negatively associated with the risk of infants' runny nose and cough symptoms but had no significant effect on any of the three mothers' symptoms. High level of humidity is negatively associated with the mothers' cough incidence but had no significant association with any of the three infants' symptoms. Such differences reveal not only the sensitivity of the mothers and infants to the air quality, but also call for further understanding of the differences. It is possible that actions taken to overcome humidity by mothers may inadvertently affect their infants.},
journal = {Journal of the American Statistical Association},
pages = {48--60},
volume = {103},
publisher = {Taylor \& Francis},
number = {481},
year = {2008},
title = {Joint Modeling of Time Series Measures and Recurrent Events and Analysis of the Effects of Air Quality on Respiratory Symptoms},
copyright = {American Statistical Association 2008},
language = {eng},
address = {ALEXANDRIA},
author = {Zhang, Heping and Ye, Yuanqing and Diggle, Peter and Shi, Jian},
keywords = {Air pollution ; Air quality ; Air quality assessment ; Applications ; Applications and Case Studies ; Censored data ; Cough ; El Niño ; Etiology ; Exact sciences and technology ; General topics ; Health ; Health risk assessment ; Humidity ; Infants ; Inference from stochastic processes; time series analysis ; Joint analysis ; Longitudinal data ; Mathematics ; Maximum likelihood estimation ; Maximum likelihood method ; Medical sciences ; Modeling ; Mothers ; Multivariate analysis ; Nose ; Outdoor air quality ; Parametric models ; Pediatrics ; Pets ; Physical Sciences ; Probability and statistics ; Recurrent event ; Regression analysis ; Respiratory care ; Respiratory diseases ; Respiratory symptoms ; Respiratory system ; Risk factor ; Risk factors ; Science \& Technology ; Sciences and techniques of general use ; Sore throat ; Standard error ; Statistical analysis ; Statistical data ; Statistical methods ; Statistical models ; Statistics ; Statistics \& Probability ; Time series ; Time series models},
}

@article{KlingenbergBernhard2008Rmfb,
issn = {0167-9473},
abstract = {Time series of discrete random variables present unique statistical challenges due to serial correlation and uneven sampling intervals. While regression models for a series of counts are well developed, only few methods are discussed for the analysis of moderate to long (e.g.Â from 20 to 152 observations) binary or binomial time series. This article suggests generalized linear mixed models with autocorrelated random effects for a parameter-driven approach to such series. We use a Monte Carlo EM algorithm to jointly obtain maximum likelihood estimates of regression parameters and variance components. The likelihood approach, although computationally extensive, allows estimation of marginal joint probabilities of two or more serial events. These are crucial in checking the goodness-of-fit, whether the model adequately captures the serial correlation and for predicting future responses. The model is flexible enough to allow for missing observations or unequally spaced time intervals. We illustrate our approach and model assessment tools with an analysis of the series of winners in the traditional boat race between the universities of Oxford and Cambridge, re-evaluating a long-held belief about the effect of the weight of the crew on the odds of winning. We also show how our methods are useful in modeling trends based on the General Social Survey database.},
journal = {Computational statistics \& data analysis},
pages = {4076--4090},
volume = {52},
publisher = {Elsevier B.V},
number = {8},
year = {2008},
title = {Regression models for binary time series with gaps},
copyright = {2008 Elsevier Ltd},
language = {eng},
address = {AMSTERDAM},
author = {Klingenberg, Bernhard},
keywords = {Autocorrelation ; Computer Science ; Computer Science Interdisciplinary Applications ; Econometrics ; Exact sciences and technology ; Expectationâ€“maximization algorithm ; General topics ; Generalized linear mixed model ; Likelihood function ; Linear inference regression ; Linear model ; Mathematics ; Mixed model ; Multivariate analysis ; Numerical analysis ; Numerical analysis. Scientific computation ; Numerical methods in probability and statistics ; Physical Sciences ; Probability and statistics ; Random effects model ; Regression analysis ; Science \& Technology ; Sciences and techniques of general use ; Statistics ; Statistics \& Probability ; Technology},
}

@article{LiJinglan2019Htmm,
issn = {1048-5252},
abstract = {We propose time-varying coefficient mixed-effects models for continuous multiple time series data and longitudinal data. The challenge is how to simultaneously display serial, clustering, and multivariate attributes of the data set, to which the routinely assumed two-level hierarchical model and univariate response models are not able to apply. Asymptotic properties of the proposed methods are established. We also conduct the model comparison, and find that the proposed methods outperform the traditional univariate response models, nonparametric models, and linear mixed effects models in both predicting the response and estimating the coefficient surface based on simulation studies. Finally, we have applied our methods to a real-world study on the price-volume relationship of NASDAQ stock market data.},
journal = {Journal of nonparametric statistics},
pages = {695--721},
volume = {31},
publisher = {Taylor \& Francis},
number = {3},
year = {2019},
title = {Hierarchical time-varying mixed-effects models in high-dimensional time series and longitudinal data studies},
copyright = {American Statistical Association and Taylor \& Francis 2019 2019},
language = {eng},
address = {Abingdon},
author = {Li, Jinglan and Zhang, Zhengjun},
keywords = {Asymptotic methods ; Asymptotic properties ; Clustering ; Computer simulation ; local linear regression ; mixed effects models ; Multivariate analysis ; profile likelihood ; Time series ; Varying coefficient models},
}

@article{ChoSun-Joo2018AGLM,
